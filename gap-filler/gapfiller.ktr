        <class_name>Processor</class_name>

        <class_source><![CDATA[import java.util.*;

Date eventEnd;
Date nextEvent;

FieldHelper eventEndHelper;
FieldHelper nextEventHelper;
FieldHelper eventTimeHelper;
FieldHelper gapRecordHelper;
FieldHelper previousEventEndHelper;

Object[] r;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	if (first){
	  first = false;

	  eventEndHelper = get(Fields.In, "eventEndTime");
	  nextEventHelper = get(Fields.In, "nextStartDate");
	  eventTimeHelper = get(Fields.In, "eventTime");
	  previousEventEndHelper = get(Fields.In, "previousEndDate");

	  gapRecordHelper = get(Fields.Out, "gapRecord");
	}

	r = getRow();

	if (r == null) {
	  setOutputDone();
	  return false;
	}

	// It is always safest to call createOutputRow() to ensure that your output row's Object[] is large
    // enough to handle any new fields you are creating in this step.
    r = createOutputRow(r, data.outputRowMeta.size());
    
    // Get the value from an input field
    eventEnd = eventEndHelper.getDate(r);
    nextEvent = nextEventHelper.getDate(r);

	// Set the flag so we can see if its not one we added
	gapRecordHelper.setValue(r, false);

	// Send the row on to the next step.  We're detecting a gap AFTER this row, so send it on first before filling.
    putRow(data.outputRowMeta, r);

	if ( eventEnd!=null && nextEvent != null && nextEvent.after(eventEnd) ) {

		// For the new row, the start date has to be the previous end date, and the end date is the next start date.

		Object[] r2 = createOutputRow(r, data.outputRowMeta.size());
		eventTimeHelper.setValue(r2, eventEnd);
		eventEndHelper.setValue(r2, nextEvent);		

		// Set the flag so we can see if its not one we added
		gapRecordHelper.setValue(r2, true);

